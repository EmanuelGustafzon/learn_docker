services:
  db:
    container_name: products_db
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: Password123!
    image: mcr.microsoft.com/mssql/server:2022-latest
    networks:
      my-network: null
    ports:
      - mode: ingress
        target: 1433
        published: "1433"
        protocol: tcp
    restart: always
    volumes:
      - type: volume
        source: products-volume
        target: /var/opt/mssql
        volume: {}
  webapi:
